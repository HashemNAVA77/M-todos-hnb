package metodos;

/**
 *
 * @author HNB
 */
public class Metodos hnb {
public static int sumar(int a, int b){
    return a+b;
}
public static int resta(int a, int b){
    return a-b;
}
public static int divicion(int a, int b){
    return a/b;
}
public static int multiplicacion(int a, int b){
    return a*b;
}
    /**
     * @param args the command line arguments
     */
    public static void main(String[] args) {
        int resultado =Metodos.sumar(8,2);
        System.out.println("suma es:"+ resultado);
         int resula =Metodos.resta(7,2);
        System.out.println("resta es:" + resula);
         int resl=Metodos.divicion(3,2);
        System.out.println("divicion es:"+ resl);
         int resultadoss =Metodos.multiplicacion(9,2);
        System.out.println("multiplicacion  es:"+ resultadoss);
        
        // TODO code application logic here
    }
    
}
